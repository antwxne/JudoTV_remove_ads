{
    "manifest_version": 3,
    "name": "JUDO TV",
    "description": "Remove ads from JUDO TV video player",
    "version": "1.0",
    "content_scripts": [
        {
            "js": [
                "script.js"
            ],
            "matches": [
                "https://judotv.com/*/live"
            ],
            "run_at": "document_end"
        }
    ]
}